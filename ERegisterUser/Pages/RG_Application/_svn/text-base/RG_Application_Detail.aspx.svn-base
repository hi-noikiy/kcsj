<%@ Page Language="C#" MasterPageFile="~/WebMaster/ContentPageNoTop.master" AutoEventWireup="true"
    Inherits="EpointRegisterUser.Pages.RG_Application.RG_Application_Detail" Title="应用子系统明细"
    CodeBehind="RG_Application_Detail.aspx.cs" %>

<%@ Register Assembly="Epoint.Web.UI.WebControls2X" Namespace="Epoint.Web.UI.WebControls2X.TextBoxControls"
    TagPrefix="epoint" %>
<%@ Register Assembly="Epoint.Web.UI.WebControls2X" Namespace="Epoint.Web.UI.WebControls2X"
    TagPrefix="epoint" %>
<%@ Register TagPrefix="webdiyer" Namespace="Wuqi.Webdiyer" Assembly="AspNetPager" %>
<asp:Content ID="Content1" ContentPlaceHolderID="ContentPlaceHolder1" runat="server">
    <table cellspacing="0" cellpadding="0" width="100%" border="0">
        <tr>
            <td id="tdContainer" valign="top" align="center" runat="server">
                <table width="100%" cellspacing="1">
                    <tr>
                        <td class="TableSpecial1" width="15%">
                            系统名称:
                        </td>
                        <td class="TableSpecial" width="35%" height="26">
                            <asp:Label ID="AppName_118" Width="100%" runat="server"></asp:Label>
                        </td>
                    </tr>
                    <tr>
                        <td class="TableSpecial1" width="15%">
                            系统类别:
                        </td>
                        <td class="TableSpecial" width="35%" height="26">
                            <asp:Label ID="AppType_118" Width="100%" runat="server"></asp:Label>
                        </td>
                    </tr>
                    <tr>
                        <td class="TableSpecial1" width="15%">
                            系统URL:
                        </td>
                        <td class="TableSpecial" width="35%" height="26">
                            <asp:Label ID="AppUrl_118" Width="100%" runat="server"></asp:Label>
                        </td>
                    </tr>
                    <tr>
                        <td class="TableSpecial1" width="15%">
                            是否禁用:
                        </td>
                        <td class="TableSpecial" width="35%" height="26">
                            <asp:Label ID="IsForbid_118" Width="100%" runat="server"></asp:Label>
                        </td>
                    </tr>
                    <tr>
                        <td class="TableSpecial1" width="15%">
                            是否弹出新窗口:
                        </td>
                        <td class="TableSpecial" width="35%" height="26">
                            <asp:Label ID="IsBlank_118" Width="100%" runat="server"></asp:Label>
                        </td>
                    </tr>
                    <tr>
                        <td class="TableSpecial1" width="15%">
                            快捷菜单图片:
                        </td>
                        <td class="TableSpecial" width="35%" height="26">
                            <asp:Label ID="ShortcutImg_118" Width="100%" runat="server"></asp:Label>
                        </td>
                    </tr>
                    <tr>
                        <td class="TableSpecial1" width="15%">
                            快捷菜单文字:
                        </td>
                        <td class="TableSpecial" width="35%" height="26">
                            <asp:Label ID="ShortcutText_118" Width="100%" runat="server"></asp:Label>
                        </td>
                    </tr>
                    <tr>
                        <td class="TableSpecial1" width="15%">
                            排序
                        </td>
                        <td class="TableSpecial" width="35%" height="26" align="left">
                            <asp:Label ID="OrderNum_118" runat="server"></asp:Label>
                        </td>
                    </tr>
                    <tr>
                        <td class="TableSpecial1" width="15%">
                            应用系统管理员:
                        </td>
                        <td class="TableSpecial" width="35%" height="26">
                            <asp:Label ID="AppManager" Width="100%" runat="server"></asp:Label>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
        <tr>
            <td align="center" colspan="4" height="40">
                <asp:Label ID="lblMessage" runat="server" ForeColor="Red" Visible="False">没有数据！</asp:Label>
            </td>
        </tr>
    </table>
</asp:Content>
